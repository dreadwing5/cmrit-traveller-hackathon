    "></script>
    <script src="/socket.io/socket.io.js"></script>
    <script defer src="/js/script.js"></script>

    <title>CMRIT TRAVELLER</title>
</head>

<body>
    <div class="sidebar">
        <ul class="workouts">
            <form class="form">
                <div class="form__row">
                    <label class="form__label">Source</label>
                    <input class="form__input form__input--source" />
                </div>
                <div class="form__row">
                    <label class="form__label">Destination</label>
                    <input class="form__input form__input--destination" />
                </div>
                <div class="form__row">
                    <label class="form__label">Date</label>
                    <input type="date" class="form__input form__input--date" />
                </div>
                <div class="form__row ">
                    <label class="form__label">Time</label>
                    <input type="time" class="form__input form__input--time" />
                </div>
                <button type="submit" class="form__btn btn">Submit</button>
            </form>
        </ul>

    </div>

    <div id="map"></div>
</body>